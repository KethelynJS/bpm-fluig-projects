function enableFields(form){
	var atv_atual = getValue("WKNumState");
	
	log.info("enableFields - atv_atual: " + atv_atual);
	
	if(atv_atual == 4) {
		
	}
	
	
	else if(atv_atual == 5){
		desabilitarSolicitante5(form);
	}
	else if (atv_atual == 9) {
		desabilitarCamposAtividade9}
	
    else if (atv_atual == 11) {
    	desabilitarCamposAtividade11;  
    }
 
	

	

function desabilitarSolicitante5(form){
	form.setEnabled("lote", false);
	form.setEnabled("itens", false);
	form.setEnabled("aspecto", false);
	form.setEnabled("Resp", false);
	form.setEnabled("bacteriana", false);
	form.setEnabled("estufa", false);
	
	
}

function desabilitarCamposAtividade9(form) {
	form.setEnabled("lote", false);
	form.setEnabled("itens", false);
	form.setEnabled("aspecto", false);
	form.setEnabled("Resp", false);
	form.setEnabled("bacteriana", false);
	form.setEnabled("estufa", false);
	form.setEnabled("datainicio2", false);
	form.setEnabled("Resp4", false);
	
}

function desabilitarCamposAtividade11(form) {
	form.setEnabled("lote", false);
	form.setEnabled("itens", false);
	form.setEnabled("aspecto", false);
	form.setEnabled("Resp", false);
	form.setEnabled("bacteriana", false);
	form.setEnabled("estufa", false);
	form.setEnabled("datainicio2", false);
	form.setEnabled("Resp4", false);
	form.setEnabled("datacafinall", false);
	form.setEnabled("opcao", false);
	form.setEnabled("respc", false);
	form.setEnabled("obs_esterilidade", false);
	
}
}